{
    "configurations": [
        {
            "name": "ESP-IDF",
            "compilerPath": "${config:idf.toolsPath}/tools/xtensa-esp-elf/esp-13.2.0_20240530/xtensa-esp-elf/bin/xtensa-esp32s3-elf-gcc",
            "compileCommands": "${config:idf.buildPath}/compile_commands.json",
            "includePath": [
                "${config:idf.espIdfPath}/components/**",
                "${config:idf.espIdfPathWin}/components/**",
                "${workspaceFolder}/**",
                "${config:idf.espIdfPath}/components/soc/esp32s3/include",
                "${config:idf.espIdfPath}/components/esp32s3/**"
            ],
            "browse": {
                "path": [
                    "${config:idf.espIdfPath}/components",
                    "${config:idf.espIdfPathWin}/components",
                    "${workspaceFolder}",
                    "${config:idf.espIdfPath}/components/soc/esp32s3/include",
                    "${config:idf.espIdfPath}/components/esp32s3"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "defines": [
                "ESP_PLATFORM",
                "CONFIG_IDF_TARGET_ESP32S3"
            ]
        }
    ],
    "version": 4
}
